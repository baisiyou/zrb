<Window x:Class="LabProblem3.PrimeDetection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabProblem3"
        mc:Ignorable="d"
        Title="PrimeDetection" Height="450" Width="800">
    <Grid>
        <Button Content="Change Number" HorizontalAlignment="Left" Height="38" Margin="83,102,0,0" VerticalAlignment="Top" Width="115" FontSize="14" Background="{x:Null}" BorderBrush="{x:Null}"/>
        <TextBox x:Name="primeInput" HorizontalAlignment="Left" Height="28" Margin="262,112,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" FontSize="14"/>
        <Button x:Name="Prime_Detection" Content="Detect Prime" HorizontalAlignment="Left" Height="38" Margin="285,274,0,0" VerticalAlignment="Top" Width="115" FontSize="14" Click="Prime_Detection_Click"/>
        <Label x:Name="result" Content="label" HorizontalAlignment="Left" Height="52" Margin="276,0,0,0" VerticalAlignment="Center" Width="186" FontSize="16" IsEnabled="False"/>

    </Grid>
</Window>
